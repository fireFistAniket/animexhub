{
  "version": 2,
  "builds": [
    {
      "src": "Backend/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/package.json"
    }
  ]
}
